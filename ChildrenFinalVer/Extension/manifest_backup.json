{
  "name": "Privacy Panda",
  "description": "Monitoring Consent on Children's Sites",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "128": "./icon.png",
    "64": "./panda_icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "incognito": "split",
  "background": {
    "_comment1": "these are the scripts to run",
    "scripts": [
      "./background.js"
    ]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_icon": "panda_icon.png",
    "default_popup": "popup.html",
    "default_title": "Privacy Panda"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}